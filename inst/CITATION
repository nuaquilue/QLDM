         author = "Mathieu, Núria, Élise, Jonathan, Marc-André",
         journal = "CJFR",
         year = "20XX",
         textVersion =
         paste("Bourchard, M., Aquilué, N., Filotas, É., Boucher, J., Parisien, M.-A. (20xx). ",
               "Anticipating the impacts of climate change on wildfire-induced declines in harvest levels across Quebec’s forests. ",
               "CJFR",
               ", DOI nnnn",
               sep=""),
         )

